<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>CarConfigurator.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
  <servlet-name>serviceImpl</servlet-name>
  <servlet-class>edu.hm.carconfigurator.server.ServiceImpl</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>serviceImpl</servlet-name>
  <url-pattern>/edu.hm.carconfigurator.CarConfigurator/MyService</url-pattern>
</servlet-mapping>

</web-app>
